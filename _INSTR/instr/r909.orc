#define if_r909(array'dyn) #
kdyn_$array = gkr909_$array[k16th%lenarray(gkr909_$array)]
if kdyn_$array >= 1 then

	if gkr909_stopper > -.35 then
		schedulek "r909_instrument", 0, 1, idyn*kdyn_$array/$dyn, ich, gSr909_$array[random:k(0, lenarray(gSr909_$array))]
	endif

	if random:k(0, 1) > .895 then
		kvar = floor(random:k(6, 13))
		schedulek "r909_instrument", gkbeats/kvar, 1, idyn*kdyn_$array/$dyn/kvar, ich, gSr909_$array[random:k(0, lenarray(gSr909_$array))]
	endif

endif
#

gkr909_stopper init 0

$start_instr(r909)
	idiv			init icps%i(gkdiv)
	kdiv_bar		init -1
	ksubdiv 		init -1

	if active:i("r909_stopper") == 0 then
		schedule "r909_stopper", 0, idur
	endif

	; divide the main 64 quarter notes into a subdivision of 8 quarter notes
	; so from 0 to 1 now the cycle is from 0 to (64 / 8) = 8
	; representing the 8 bars
	kcycle			chnget "heart"
	kdiv_bar		= kcycle * divz(gkdiv, idiv, 1)


	; in order to get each quarter notes subdivision of 32
	; get the modulo 1 of the cycle and then multiply by 8
	ksubdiv	= (kdiv_bar % 1) * idiv

	; 4 for 16th, 8 for 32th..
	;k16th	= floor( (ksubdiv % 1) * 4 ) + 4 * floor( (gkHEART * gkdiv) )
	k16th	= floor( (ksubdiv % 1) * idiv ) + idiv * floor( (kcycle * gkdiv) )

	if changed2(k16th) == 1 then
		$if_r909(cym_crash'4)
		$if_r909(cym_ride'16)
		$if_r909(hh_open'24)
		$if_r909(hh_closed'12)
		$if_r909(hh_clop'12)
		$if_r909(hh_opcl'12)
		$if_r909(snare'3)
		$if_r909(snare_rimshot'2)
		$if_r909(snare_handclap'4)
		$if_r909(tom_low'2)
		$if_r909(tom_mid'2)
		$if_r909(tom_high'2)
		$if_r909(bass_2'2)
		$if_r909(bass_1'2)
	endif

endin

instr r909_stopper
	gkr909_stopper  jitter 1, gkbeatf/4, gkbeatf*4
endin

instr r909_instrument
	Sinstr 	init "r909"
	idyn 	init p4
	ich		init p5
	Spath 	init p6

	ilen filelen Spath
	p3 init ilen

